{
  "name": "react-formation-stripe-example",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all build:**",
    "start": "npm run build && npm-run-all --parallel server watch:**",
    "prebuild": "rimraf www && mkdirp www",
    "prestart": "npm run prebuild",
    "build:static": "cpx \"src/static/**/*\" www",
    "watch:static": "npm run build:static -- -w",
    "build:js": "webpack",
    "watch:js": "npm run build:js -- --watch",
    "build:css": "npm run watch:css -- --no-watch",
    "watch:css": "autoless --source-map src www",
    "server": "live-server www --port=7777"
  },
  "author": "pippinlee",
  "license": "MPL-2.0",
  "dependencies": {
    "classnames": "^2.1.3",
    "react": "^0.13.3",
    "react-formation": "^0.3.0"
  },
  "engines": {
    "npm": ">=2.0.0"
  },
  "devDependencies": {
    "autoless": "^0.1.7",
    "babel-core": "^5.2.17",
    "babel-loader": "^5.0.0",
    "cpx": "^1.1.6",
    "jsx-loader": "^0.13.2",
    "live-server": "^0.7.1",
    "mkdirp": "^0.5.0",
    "mocha": "^2.2.5",
    "npm-run-all": "^1.2.4",
    "rimraf": "^2.3.3",
    "should": "^7.0.4",
    "webpack": "^1.9.4"
  }
}
